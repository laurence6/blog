---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
	<title>Liu's Blog</title>
	<link href="http://blog.chit-chatting.tk/atom.xml" rel="self"/>
	<link href="http://blog.chit-chatting.tk/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://blog.chit-chatting.tk/</id>
	<author>
		<name>Liu</name>
		<email>liuxy6@gmail.com</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://blog.chit-chatting.tk{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://blog.chit-chatting.tk{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
 
</feed>